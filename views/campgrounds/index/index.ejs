<% layout('layouts/boilerplate')%>
<div class="display-3">
  All Users
</div>

</div>

<div
  class="justify-content-center jumbotron d-inline-flex align-items-center flex-md-wrap"
>
  <% for (let campground of campgrounds) {%>
  <div class="p-3 m-1 text-center card border-info">
    <img class="card-img-top" src="..." alt="Card image cap" />

    <div class="card-body">
      <h1><%= campground.Name %></h1>
      <h5 class="card-title"><%= campground.Dob %></h5>
      <p class="card-text"><%= campground.Category %></p>
      <p class="card-text">
        <small class="text-muted"><%= campground.age %> </small>
      </p>
      <a href="/home/<%= campground.id %>" class="btn btn-info btn-lg">
        View <%= campground.Name %>
      </a>
    </div>
  </div>
  <% } %>
  <a
    href="/home/new"
    class="p-2 mt-5 btn btn-outline-primary btn-block"
    style="font-size: 25px"
  >
    Add New User</a
  >
</div>
